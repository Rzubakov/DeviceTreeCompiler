<?xml version='1.0' encoding='UTF-8'?>

<faces-config
    xmlns='http://java.sun.com/xml/ns/javaee'
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:schemaLocation='http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd'
    version='2.2'>
     
    
    <!-- Pages -->
    <navigation-rule>
        <navigation-case>
            <from-outcome>convert</from-outcome>
            <to-view-id>/configurator/convert.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
        <navigation-case>
            <from-outcome>controllers</from-outcome>
            <to-view-id>/configurator/controllers.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
        <navigation-case>
            <from-outcome>modules</from-outcome>
            <to-view-id>/configurator/modules.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>chanels</from-outcome>
            <to-view-id>/configurator/chanels.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>iec</from-outcome>
            <to-view-id>/configurator/iec.xhtml</to-view-id>
            <redirect/>
        </navigation-case>   
        <navigation-case>
            <from-outcome>export</from-outcome>
            <to-view-id>/configurator/export.xhtml</to-view-id>
            <redirect/>
        </navigation-case>    
        <navigation-case>
            <from-outcome>projects</from-outcome>
            <to-view-id>/configurator/projects.xhtml</to-view-id>
            <redirect/>
        </navigation-case>   
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
            <redirect/>
        </navigation-case>   
        <navigation-case>
            <from-outcome>request</from-outcome>
            <to-view-id>/request.xhtml</to-view-id>
            <redirect/>
        </navigation-case>          
        
    </navigation-rule>


</faces-config>