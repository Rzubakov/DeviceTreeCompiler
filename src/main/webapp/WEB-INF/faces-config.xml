<?xml version='1.0' encoding='UTF-8'?>

<faces-config
    xmlns='http://java.sun.com/xml/ns/javaee'
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:schemaLocation='http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd'
    version='2.2'>
     
    
    <!-- Pages -->
    <navigation-rule>
        <navigation-case>
            <from-outcome>convert</from-outcome>
            <to-view-id>/convert.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
        <navigation-case>
            <from-outcome>controllers</from-outcome>
            <to-view-id>/controllers.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
        <navigation-case>
            <from-outcome>modules</from-outcome>
            <to-view-id>/modules.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
        <navigation-case>
            <from-outcome>iec</from-outcome>
            <to-view-id>/iec.xhtml</to-view-id>
            <redirect/>
        </navigation-case>   
    </navigation-rule>


</faces-config>